<Window x:Class="Scrap.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Scrap" Height="306" Width="433" Icon="ScrapIcon.ico">
    <Grid>
        <TabControl HorizontalAlignment="Left" Height="250" Margin="10,10,0,0" VerticalAlignment="Top" Width="400">
            <TabItem Header="ZoomBucks">
                <Grid Margin="0,1,0,-1" Height="250" IsHitTestVisible="True" Width="420" RenderTransformOrigin="0.5,0.5">
                    <Label Content="Username:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
                    <Label Content="Password:" HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top"/>
                    <TextBox x:Name="txtUsernameZoom" HorizontalAlignment="Left" Height="23" Margin="81,13,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" RenderTransformOrigin="-1.85,-4.87"/>
                    <PasswordBox x:Name="txtPasswordZoom" HorizontalAlignment="Left" Margin="81,44,0,0" VerticalAlignment="Top" Width="120" Height="23"/>
                    <Button x:Name="btnLoginZoom" Content="Start To Zoom" HorizontalAlignment="Left" Margin="81,86,0,0" VerticalAlignment="Top" Width="85" Click="btnLoginZoom_Click"/>
                    <Button x:Name="btnStopZoom" Content="Stop Zooming" HorizontalAlignment="Left" Margin="81,111,0,0" VerticalAlignment="Top" Width="85" Click="btnStopZoom_Click"/>
                    <Button x:Name="btn_openZoom" Content="Open Zoom Bucks" HorizontalAlignment="Left" Margin="219,29,0,0" VerticalAlignment="Top" Width="105" Click="btn_openZoom_Click"/>
                    <Image x:Name="image" HorizontalAlignment="Left" Height="67" Margin="124,151,0,0" VerticalAlignment="Top" Width="269" Source="Embedded/ZoomLogo.png" Stretch="None"/>
                </Grid>
            </TabItem>
            <TabItem Header="Swagbucks">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>
                    <Label Content="Username:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
                    <Label Content="Password:" HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top"/>
                    <TextBox x:Name="txtUsernameSwag" HorizontalAlignment="Left" Height="23" Margin="64,13,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" RenderTransformOrigin="-1.85,-4.87"/>
                    <PasswordBox x:Name="txtPasswordSwag" HorizontalAlignment="Left" Margin="64,44,0,0" VerticalAlignment="Top" Width="120" Height="23"/>
                    <Button x:Name="btnLoginSwag" Content="Start The Bucks" HorizontalAlignment="Left" Margin="64,86,0,0" VerticalAlignment="Top" Width="85" Click="btnLoginSwag_Click"/>
                    <Button x:Name="btnStopSwag" Content="Stop The Bucks" HorizontalAlignment="Left" Margin="64,111,0,0" VerticalAlignment="Top" Width="85" RenderTransformOrigin="-0.36,-0.3" Click="btnStopSwag_Click"/>
                    <Button x:Name="btnVideos" Content="Start Vids" HorizontalAlignment="Left" Margin="164,86,0,0" VerticalAlignment="Top" Width="75" Click="btnVideos_Click"/>
                    <Image x:Name="image1" HorizontalAlignment="Left" Height="81" Margin="114,141,0,0" VerticalAlignment="Top" Width="280" Stretch="None" Source="Embedded/SwagBucks.png"/>
                    <Button x:Name="btnOpenBucks" Content="Open The Bucks" HorizontalAlignment="Left" VerticalAlignment="Top" Width="105" Margin="202,29,0,0" Click="btnOpenBucks_Click"/>
                </Grid>
            </TabItem>
            <TabItem Header="PrizeRebel" HorizontalAlignment="Left" Height="20" VerticalAlignment="Top">
                <Grid>
                    <Label Content="Username:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
                    <Label Content="Password:" HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top"/>
                    <TextBox x:Name="txtUsernameRebel" HorizontalAlignment="Left" Height="23" Margin="81,13,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" RenderTransformOrigin="-1.85,-4.87"/>
                    <PasswordBox x:Name="txtPasswordRebel" HorizontalAlignment="Left" Margin="81,44,0,0" VerticalAlignment="Top" Width="120" Height="23"/>
                    <Button x:Name="btnLoginRebel" Content="Start Rebel" HorizontalAlignment="Left" Margin="81,86,0,0" VerticalAlignment="Top" Width="85" Click="btnLoginRebel_Click"/>
                    <Button x:Name="btnStopRebel" Content="Stop Rebel" HorizontalAlignment="Left" Margin="81,111,0,0" VerticalAlignment="Top" Width="85" RenderTransformOrigin="-0.36,-0.3" Click="btnStopRebel_Click"/>
                    <Image x:Name="image2" HorizontalAlignment="Left" Height="73" VerticalAlignment="Top" Width="307" Margin="87,149,0,0" Source="Embedded/PrizeRebel.png" Stretch="None"/>
                    <Button x:Name="btnOpenRebel" Content="Open Prize Rebel" HorizontalAlignment="Left" VerticalAlignment="Top" Width="97" Margin="217,29,0,0" Click="btnOpenRebel_Click"/>
                </Grid>
            </TabItem>
            <TabItem Header="GiftHulk" HorizontalAlignment="Left" Height="20" VerticalAlignment="Top" Width="55">
                <Grid>
                    <Label Content="Username:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
                    <Label Content="Password:" HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top"/>
                    <TextBox x:Name="txtUsernameGiftHulk" HorizontalAlignment="Left" Height="23" Margin="81,13,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" RenderTransformOrigin="-1.85,-4.87"/>
                    <PasswordBox x:Name="txtPasswordGiftHulk" HorizontalAlignment="Left" Margin="81,44,0,0" VerticalAlignment="Top" Width="120" Height="23"/>
                    <Button x:Name="btnLoginGiftHulk" Content="Start Hulking" HorizontalAlignment="Left" Margin="81,86,0,0" VerticalAlignment="Top" Width="85" Click="btnLoginGiftHulk_Click"/>
                    <Button x:Name="btnStopGiftHulk" Content="Stop Hulking" HorizontalAlignment="Left" Margin="81,111,0,0" VerticalAlignment="Top" Width="85" RenderTransformOrigin="-0.36,-0.3" Click="btnStopGiftHulk_Click"/>
                    <Image x:Name="image3" HorizontalAlignment="Left" Height="74" VerticalAlignment="Top" Width="271" Margin="123,148,0,0" Source="Embedded/GiftHulk.png" Stretch="None"/>
                    <Button x:Name="btnOpenHulk" Content="Open Gift Hulk" HorizontalAlignment="Left" VerticalAlignment="Top" Width="89" Margin="217,29,0,0" Click="btnOpenHulk_Click"/>
                </Grid>
            </TabItem>
            <TabItem Header="LootPalace" HorizontalAlignment="Left" Height="20" VerticalAlignment="Top">
                <Grid Background="#FFE5E5E5">
                    <Label Content="Username:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
                    <Label Content="Password:" HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top"/>
                    <TextBox x:Name="txtUsernamePalace" HorizontalAlignment="Left" Height="23" Margin="81,13,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" RenderTransformOrigin="-1.85,-4.87"/>
                    <PasswordBox x:Name="txtPasswordPalace" HorizontalAlignment="Left" Margin="81,44,0,0" VerticalAlignment="Top" Width="120" Height="23"/>
                    <Button x:Name="btnLoginPalace" Content="Start Looting" HorizontalAlignment="Left" Margin="81,86,0,0" VerticalAlignment="Top" Width="85" Click="btnLoginPalace_Click"/>
                    <Button x:Name="btnStopPalace" Content="Stop Looting" HorizontalAlignment="Left" Margin="81,111,0,0" VerticalAlignment="Top" Width="85" RenderTransformOrigin="-0.36,-0.3" Click="btnStopPalace_Click"/>
                </Grid>
            </TabItem>
            <TabItem Header="instaGC" HorizontalAlignment="Left" Height="20" VerticalAlignment="Top">
                <Grid Background="#FFE5E5E5">
                    <Label Content="Username:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
                    <Label Content="Password:" HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top"/>
                    <TextBox x:Name="txtUsernameGC" HorizontalAlignment="Left" Height="23" Margin="81,13,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" RenderTransformOrigin="-1.85,-4.87"/>
                    <PasswordBox x:Name="txtPasswordGC" HorizontalAlignment="Left" Margin="81,44,0,0" VerticalAlignment="Top" Width="120" Height="23"/>
                    <Button x:Name="btnLoginGC" Content="Start instaGC" HorizontalAlignment="Left" Margin="81,86,0,0" VerticalAlignment="Top" Width="85" Click="btnLoginGC_Click"/>
                    <Button x:Name="btnStopGC" Content="Stop instaGC" HorizontalAlignment="Left" Margin="81,111,0,0" VerticalAlignment="Top" Width="85" RenderTransformOrigin="-0.36,-0.3" Click="btnStopGC_Click"/>
                </Grid>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
